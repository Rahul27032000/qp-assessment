{
  "name": "assesment",
  "version": "1.0.0",
  "description": "SmartGrocery is your go-to platform for effortless online grocery management. Admins can easily update inventory, while users enjoy a user-friendly interface for viewing and booking items. With secure authentication, scalable infrastructure, and robust monitoring, SmartGrocery ensures a smooth and secure shopping experience for all.",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "rm -rf dist && tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "dockerUp": "docker-compose up --build || docker compose up --build",
    "dockerDown": "docker-compose down || docker compose down"
  },
